{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CoreApplication",
            "request": "launch",
            "mainClass": "it.smartcommunitylabdhub.core.CoreApplication",
            "projectName": "core",
        },
        {
            "type": "java",
            "name": "CoreApplication-local",
            "request": "launch",
            "mainClass": "it.smartcommunitylabdhub.core.CoreApplication",
            "projectName": "core",
            "vmArgs": "-Dspring.profiles.active=local",
            "env": {
                "K8S_NAMESPACE": "dev-platform",
                "KUBECONFIG": "/Users/raman/workspace/atlas.config",
                "RUNTIME_VLLM_SERVE_IMAGE": "smartcommunitylab/vllm:v0.11.1",
                "RUNTIME_VLLM_SERVE_CPU_IMAGE": "smartcommunitylab/vllm-cpu:v0.11.1",
                "K8S_SEC_DISABLE_ROOT": "true",
                "K8S_RESOURCE_CPU_DEFAULT": 2,
                "RUNTIME_VLLM_USER_ID": "65500",
                "RUNTIME_VLLM_GROUP_ID": "65500",
                "RUNTIME_HUGGINGFACE_SERVE_IMAGE": "kserve/huggingfaceserver:v0.14.0",
                "BUILDER_FRAMEWORK": "buildkit"
            }
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}